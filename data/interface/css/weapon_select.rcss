/*Scroll Buttons*/

div#scroll_button_wrapper
{
	position: absolute;
	height: 100%;
}

#primary_icon_list scrollbarvertical
{
	scrollchange-sound: 9;
	width: 20%;
	position: absolute;
	left:27%;
	z-index: 5;
}

#primary_icon_list scrollbarvertical slidertrack
{
	visibility: hidden;
}

#primary_icon_list scrollbarvertical sliderbar
{
	visibility: hidden;
}

#primary_icon_list scrollbarvertical sliderbar:hover
{
	
}

#primary_icon_list scrollbarvertical sliderbar:active
{
	
}

#primary_icon_list scrollbarvertical sliderarrowdec
{
	width: 100%;
	height: 10%;
	margin-top: -11%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-wide-up.png;
}

#primary_icon_list scrollbarvertical sliderarrowinc
{
	width: 100%;
	height: 10%;
	margin-bottom: -11%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-wide-down.png;
}

#primary_icon_list scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-wide-up-h.png;
}

#primary_icon_list scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-wide-down-h.png;
}

#primary_icon_list scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-wide-up-o.png;
}

#primary_icon_list scrollbarvertical sliderarrowinc:active
{
	icon-image: arrow-wide-down-o.png;
}

