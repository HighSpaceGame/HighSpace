}

button#brief_btn:hover #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-h.png;
}

button#brief_btn:active #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

button#brief_btn:checked #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

