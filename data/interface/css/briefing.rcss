body
{
	background-decorator: image;
	background-image: bg.png;
	z-index: bottom;
	z-index: -1;
}

div#brief_wrapper
{
	margin: auto;
	width: 70%;
	height: auto;
	position: relative;
}

div#brief_grid_window
{
	margin-top: 2%;
	width: 100%;
	height: auto;
	overflow: hidden;
	position: relative;
}

div#brief_grid_window img
{
	display: block;
	width: 100%;
	height: auto;
}

/*div#briefing_grid
{
	position: absolute;
	top: 54%;
	left: 11.5%;
	width: 100%;

}*/

div#briefing_grid
{
	position: absolute;
	top: 15.5%;
	left: 7.2%;
	width: 87.7%;
	height: 83%;

}

div#brief_grid_cut
{
	position: absolute;
	top: 15.5%;
	left: 7.2%;
	width: 100%;

}

div#brief_grid_cut ani
{

	width: 87.7%;

}

div#brief_text_window
{
	width: 90%;
	height: auto;
	overflow: hidden;
	position: relative;
	margin-top: 2%;
}

div#brief_text_window img
{
	display: block;
	width: 100%;
	height: auto;
}

div#brief_text_wrapper
{
	position: absolute;
	margin-top: 4%;
	margin-left: 7%;
	width: 88%;
}

div#brief_text
{
	line-height: 90%;
	height: 85%;
	overflow: scroll;
}

div#briefing_goals
{
	width: 100%;
	height: auto;
	position: absolute;
	top: 17%;
	left: 5%;
}

/*hide bonus goals by default*/
#bonus_goals
{
	visibility: hidden;
}

#primary_goal_list
{
	margin-left: 10%;
	height: auto;
}

#secondary_goal_list
{
	margin-left: 10%;
}

#bonus_goal_list
{
	margin-left: 10%;
}

#goal_wrapper
{
	display: block;
}

#primary_goals_header
{
	width: 94%;
	margin-bottom: 3%;
	overflow: hidden;
}

#secondary_goals_header
{
	width: 94%;
	margin-top: 5%;
	margin-bottom: 3%;
}

#bonus_goals_header
{
	width: 94%;
	margin-top: 5%;
	margin-bottom: 3%;
}

#primary_goal_text
{
	width: 17%;
	float: left;
}

#secondary_goal_text
{
	width: 19%;
	float: left;
}

#bonus_goal_text
{
	width: 16%;
	float: left;
}

.goal
{
	margin: 1%;
}

.goalsdot
{
	width: 2%;
	height: 4%;
	float: left;
	margin-right: 3.5%;
}

.leftline
{
	width: 15%;
	height: auto;
	float: left;
	margin-right: 1%;
	margin-top: 1.5%;
}

.rightline
{
	height: auto;
	float: left;
	margin-right: 1%;
	margin-top: 1.5%;
	overflow: hidden;
}

.primary_rightline
{
	width: 61%;
}

.secondary_rightline
{
	width: 59%;
}

.bonus_rightline
{
	width: 62%;
}

#goalsdot_img
{
	background-decorator: image;
	background-image: scroll-button.png;
}

#goalsline_img
{
	background-decorator: image;
	background-image: scroll-bar-line-horz.png;
	height: 0.4%;
	overflow: hidden;
}

div#brief_stage_text
{
	text-align: left;
	width: 15%;
	float: left;
	margin-left: 40%;
}

div#mission_title
{
	text-align: right;
	width: 40%;
	float: right;
}

div#mission_debug_info
{
	text-align: left;
	position: absolute;
	top: -4%;
	left: 3%;
}

div#brief_stage_wrapper
{
	position: absolute;
	top: 4%;
}

div#stage_select
{
	position: absolute;
	top: 0;
}

/*Arrow Buttons*/

div#arrow_cont
{
	float: left;
	width: 7%;
	height: auto;
	margin-left: 2%;
	margin-bottom: 2%;
	margin-top: 1%;
}

div#skip_cont
{
	float: left;
	width: 4%;
	height: auto;
	margin-left: 2%;
	margin-bottom: 2%;
	margin-top: 2%;
}

div#pause_cont
{
	float: left;
	width: 3%;
	height: auto;
	margin-left: 2%;
	margin-bottom: 2%;
	margin-top: 2%;
}

.arrowbuttons
{
	display: block;
	width: 100%;
	height: auto;

}

#cmdlast_img
{
	background-decorator: image;
	background-image: skip-r.png;
}

#cmdprev_img
{
	background-decorator: image;
	background-image: arrow-left.png;
}

#cmdpause_img
{
	background-decorator: image;
	background-image: pause.png;
}

#cmdnext_img
{
	background-decorator: image;
	background-image: arrow-right.png;
}

#cmdfirst_img
{
	background-decorator: image;
	background-image: skip-l.png;
}

button#cmdlast_btn:hover #cmdlast_img
{
	background-decorator: image;
	background-image: skip-r-h.png;
}

button#cmdprev_btn:hover #cmdprev_img
{
	background-decorator: image;
	background-image: arrow-left-h.png;
}
button#cmdpause_btn:hover #cmdpause_img
{
	background-decorator: image;
	background-image: pause-h.png;
}

button#cmdnext_btn:hover #cmdnext_img
{
	background-decorator: image;
	background-image: arrow-right-h.png;
}

button#cmdfirst_btn:hover #cmdfirst_img
{
	background-decorator: image;
	background-image: skip-l-h.png;
}

button#cmdlast_btn:active #cmdlast_img
{
	background-decorator: image;
	background-image: skip-r-o.png;
}

button#cmdprev_btn:active #cmdprev_img
{
	background-decorator: image;
	background-image: arrow-left-o.png;
}

button#cmdpause_btn:active #cmdpause_img
{
	background-decorator: image;
	background-image: pause-o.png;
}

button#cmdpause_btn:checked #cmdpause_img
{
    background-decorator: image;
    background-image: pause-o.png;
}

button#cmdnext_btn:active #cmdnext_img
{
	background-decorator: image;
	background-image: arrow-right-o.png;
}

button#cmdfirst_btn:active #cmdfirst_img
{
	background-decorator: image;
	background-image: skip-l-o.png;
}

/*Top Panel Elements*/

div#top_panel_wrapper
{
	width: 100%;
	height: auto;
	overflow: hidden;

}

div#top_panel_a
{
	width: 10%;
	height: auto;
	position: relative;
	float: right;
}

div#top_panel_a img
{
	display: block;
	width: 100%;
	height: auto;
}

div#top_panel_b
{
	width: 4%;
	height: auto;
	position: relative;
	float: left;
}

div#top_panel_b img
{
	display: block;
	width: 100%;
	height: auto;

}

/*Skip M Button*/

div#top_a_panel_wrapper
{
	position: absolute;
	top: 0;
}

div#skip_m_cont
{
	width: 38%;
	height: auto;
	margin: auto;

}

.skip
{
	display: block;
	width: 100%;
	height: auto;

}

#skip_m_text
{
	position: absolute;
	top: 100%;
	left: -50%;
	margin-right: 50%;
}

#skip_m_text p
{
	padding-top: 15%;
}

#skip_img
{
	background-decorator: image;
	background-image: skip-r.png;
}

button#skip_btn:hover #skip_img
{
	background-decorator: image;
	background-image: skip-r-h.png;
}

button#skip_btn:active #skip_img
{
	background-decorator: image;
