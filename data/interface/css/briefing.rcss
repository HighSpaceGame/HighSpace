/*Scroll Buttons*/

#brief_text scrollbarvertical
{
	scrollchange-sound: 9;
	width: 8%;
	position: absolute;
	top: 15%;
	left: -8.6%;
	z-index: 5;
}

#brief_text scrollbarvertical slidertrack
{
	visibility: hidden;
}

#brief_text scrollbarvertical sliderbar
{
	width: 55%;
	height: 5%;
	visibility: hidden;
}

#brief_text scrollbarvertical sliderbar:hover
{
	visibility: hidden;
}

#brief_text scrollbarvertical sliderbar:active
{
	visibility: hidden;
}

#brief_text scrollbarvertical sliderarrowdec
{
	width: 100%;
	height: 42%;
	margin-top: 2%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-up.png;
}

#brief_text scrollbarvertical sliderarrowinc
{
	width: 100%;
	height: 42%;
	margin-bottom: 12%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-down.png;
}

#brief_text scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-dbl-up-h.png;
}

#brief_text scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-dbl-down-h.png;
}

#brief_text scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-dbl-up-o.png;
}

#brief_text scrollbarvertical sliderarrowinc:active
{
	icon-image: arrow-dbl-down-o.png;
}
