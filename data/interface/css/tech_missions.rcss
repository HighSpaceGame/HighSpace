/*Scroll Buttons*/

#mission_list scrollbarvertical
{
	scrollchange-sound: 9;
	width: 5%;
	position: absolute;
	top: -7.5%;
	left: -7.6%;
	z-index: 5;
}

#mission_list scrollbarvertical slidertrack
{

}

#mission_list scrollbarvertical sliderbar
{
	width: 55%;
	height: 5%;
}

#mission_list scrollbarvertical sliderbar:hover
{
	
}

#mission_list scrollbarvertical sliderbar:active
{
	
}

#mission_list scrollbarvertical sliderarrowdec
{
	width: 100%;
	height: 12%;
	margin-top: 2%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-up.png;
}

#mission_list scrollbarvertical sliderarrowinc
{
	width: 100%;
	height: 12%;
	margin-bottom: -8%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-down.png;
}

#mission_list scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-dbl-up-h.png;
}

#mission_list scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-dbl-down-h.png;
}

#mission_list scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-dbl-up-o.png;
}

#mission_list scrollbarvertical sliderarrowinc:active
{
	icon-image: arrow-dbl-down-o.png;
}

/*Tooltip placements*/

div#bot_left_wrapper .tooltip
{
	width: 500%;
