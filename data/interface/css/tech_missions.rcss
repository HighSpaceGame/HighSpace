	overflow: hidden;
}

div#logo_wide img
{
	display: block;
	width: 100%;
	height: auto;
}

div#simulator_panel_wrapper
{
	position: relative;
	width: 80%;
	height: auto;
	margin: auto;
	margin-top: 1%;
}

div#simulator_window_wrapper
{
	width: 90%;
	height: auto;
	overflow: hidden;
	position: relative;
}

div#simulator_window_wrapper img
{
	display: block;
	width: 100%;
	height: auto;
}

div#column_titles
{
	position: absolute;
	top: -3%;
	margin-top: 2%;
	margin-left: 6%;
	overflow: auto;
}

div#mission_header
{
	float: left;
	width: 50%;
	position: relative;
}

div#filename_header
{
	width: 50%;
	position: relative;
	float: right;
}

div#campaign_title
{
	float: left;
	width: 50%;
	position: relative;
}

div#campaign_file
{
	width: 50%;
	position: relative;
	float: right;
}

div#mission_title
{
	float: left;
	width: 50%;
	position: relative;
}

div#mission_file
{
	width: 50%;
	position: relative;
	float: right;
}

div#campaign_name_wrapper
{
	position: absolute;
	top: 2%;
	margin-top: 2%;
	margin-left: 6%;
	overflow: auto;
	height: 90%;
}

div#mission_list
{
	position: absolute;
	top: 5%;
	margin-top: 2%;
	margin-left: 7%;
	overflow: scroll;
	height: 89%;
}

.missionlist_element
{
	display: inline-block;
	width: 100%;
	color: #7b7b7b;
	z-index: 3;
	overflow: hidden;
}
.missionlist_element:hover
{
	color: #abbbab;
}
.missionlist_element:checked
{
	color: #6f92f5;
}

.missionlist_name
{
	display: inline-block;
	width: 20%;

}

.missionlist_author
{
	display: inline-block;
	width: 19%;
	margin-left: 1%;
	white-space: pre;
	overflow: hidden;
}

.missionlist_filename
{
	display: inline-block;
	width: 19%;
	margin-left: 1%;
	overflow: hidden;
}

.missionlist_description
{
	display: inline-block;
	width: 38%;
	margin-left: 1%;
	white-space: pre;
	overflow: scroll;
}

#mission_list .missionlist_description scrollbarhorizontal
{
	display: none;
}

#mission_list .missionlist_description scrollbarvertical
{
	display: none;
}

/*Bottom Left Panel Elements*/

div#bot_left_panel_wrapper
{
	width: 100%;
	height: auto;
	overflow: hidden;
	position: absolute;
	bottom: 0;
	left: 0;
}

div#bot_left_panel
