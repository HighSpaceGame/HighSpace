body
{
	background-decorator: image;
	background-image: bg.png;
	z-index: bottom;
	z-index: -1;
}

.main_bg
{
	background-decorator: image;
	background-image: bg.png;
}

.below p
{
	padding-top:8%;
}

div#logo_wide
{
	position: absolute;
	bottom: 1%;
	margin: auto;
	width: 35%;
	height: auto;
	overflow: hidden;
}

div#logo_wide img
{
	display: block;
	width: 100%;
	height: auto;
}

div#medals_text
{
	position: absolute;
	top: 16.5%;
	left: 35%;
	color: grey;
	width: 30%;
	height: 3%;
	text-align: center;
	z-index: 1;
}

div#medals_wrapper
{
	margin: auto;
	width: 100%;
	height: auto;
}

div#medals_wrapper_actual
{
	width: 90%;
	height: auto;
}

div#medals_case
{
	width: 90%;
	height: auto;
}

div#medals_case img
{
	position: absolute;
	width: 100%;
	height: auto;
}



