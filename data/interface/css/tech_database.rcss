/*Scroll Buttons*/

#tech_list scrollbarvertical
{
	scrollchange-sound: 9;
	width: 20%;
	position: absolute;
	top: -8.5%;
	left: -24.0%;
	z-index: 5;
}

#tech_list scrollbarvertical slidertrack
{
	visibility: hidden;
}

#tech_list scrollbarvertical sliderbar
{
	width: 55%;
	height: 7.5%;
}

#tech_list scrollbarvertical sliderbar:hover
{
	
}

#tech_list scrollbarvertical sliderbar:active
{
	
}

#tech_list scrollbarvertical sliderarrowdec
{
	width: 100%;
	height: 18%;
	margin-top: 2%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-up.png;
}

#tech_list scrollbarvertical sliderarrowinc
{
	width: 100%;
	height: 18%;
	margin-bottom: -12%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-down.png;
}

#tech_list scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-dbl-up-h.png;
}

#tech_list scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-dbl-down-h.png;
}

#tech_list scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-dbl-up-o.png;
}

#tech_list scrollbarvertical sliderarrowinc:active
{
	icon-image: arrow-dbl-down-o.png;
}

