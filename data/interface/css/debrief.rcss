body
{
	background-decorator: image;
	background-image: bg.png;
	z-index: bottom;
	z-index: -1;
}

.below p
{
	padding-top:8%;
}

div#debrief_text
{
	position: absolute;
	top: 0;
	margin-top: 2%;
	margin-left: 10%;
	margin-right: 7%;
	color: grey;
	overflow: scroll;
	height: 85%;
	line-height: 90%;
}

.debrief_text_actual
{
	margin-top: 4%;
	margin-bottom: 1%;
}

.recommendation
{
	margin: 1%;
}

div#debrief_wrapper
{
	position: absolute;
	bottom: 0;
	left: 10%;
	width: 50%;
	height: auto;
}

div#debrief_text_window
{
	margin-left: 2%;
	width: 100%;
	height: auto;
	overflow: hidden;
	position: relative;
}

div#debrief_text_window img
{
	display: block;
	width: 100%;
	height: auto;
}

div#mission_name
{
	text-align: left;
	color: white;
	margin-left: 8%;
}

div#stage_select
{
	position: absolute;
	bottom: 0;
	right: 0;
	width: 50%;
}

.stats_header
{
	margin-top: 5%;
	margin-bottom: 5%;
}

.stats_subheader
{
	margin-bottom: 5%;
}

.stats_header_left
{
	float: left;
	width: 50%;
	text-align: left;
}

.stats_header_right
{
	float: right;
	width: 50%;
	text-align: right;
}

.stats_left
{
	float: left;
	width: 75%;
	text-align: left;
}

.stats_right
{
	float: right;
	width: 25%;
	text-align: left;
}

/*Replay Buttons*/

div#replay_wrapper
{
	position: absolute;
	bottom: 2%;
	width: 7%;
	padding-left: 0.3%;
	z-index: 7;
}

div#replay_cont
{
	width: 100%;
	height: auto;
	margin: auto;
	margin-top: 1%;
}

div#recommend_cont
{
	width: 100%;
	height: auto;
}

.replay_btn
{
	display: block;
	width: 100%;
	height: auto;
}

.recommend_btn
{
	display: block;
	width: 100%;
	height: auto;
}

#replay_img
{
	background-decorator: image;
	background-image: replay.png;
}

button#replay_btn:hover #replay_img
{
	background-decorator: image;
	background-image: replay-neg-h.png;
}

button#replay_btn:active #replay_img
{
	background-decorator: image;
	background-image: replay-neg-o.png;
}

#recommend_img
{
	background-decorator: image;
	background-image: bullet-r.png;
}

button#recommend_btn:hover #recommend_img
{
	background-decorator: image;
	background-image: bullet-r-h.png;
}

button#recommend_btn:active #recommend_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

/*Top Panel Elements*/

div#top_panel_wrapper
{
	width: 100%;
	height: auto;
	overflow: hidden;
}

div#top_panel_b
{
	width: 4%;
	height: auto;
	position: relative;
	float: left;
}

div#top_panel_b img
{
	display: block;
	width: 100%;
	height: auto;

}

/*Top B Panel Buttons*/

div#top_b_panel_wrapper
{
	position: absolute;
	margin-top: 5%;
	padding-top: 25%;
}

.button_text_right p
{
	padding-top: 25%;
	width: 400%;
	margin-left: 100%;
}

/*Bullet Button*/

div#bullet_cont
{
	width: 90%;
	height: auto;
	margin: auto;

}

.bullet
{
	display: block;
	width: 100%;
	height: auto;

}

#bullet_img
{
	background-decorator: image;
	background-image: bullet-r.png;
}

button#debrief_btn:hover #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-h.png;
}

button#debrief_btn:active #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

button#debrief_btn:checked #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

button#stats_btn:hover #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-h.png;
}

button#stats_btn:active #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

button#stats_btn:checked #bullet_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

/*Arrow Buttons*/

div#arrow_cont
{
	float: right;
	width: 14%;
	height: auto;
	margin-right: 4%;
	margin-bottom: 0.0%;
	margin-top: 9%;
}

div#skip_cont
{
	float: right;
	width: 8%;
	height: auto;
	margin-right: 4%;
	margin-bottom: 0.0%;
	margin-top: 10%;
}

.arrowbuttons
{
	display: block;
	width: 100%;
	height: auto;

}

#cmdlast_img
{
	background-decorator: image;
	background-image: skip-r.png;
}

#cmdprev_img
{
	background-decorator: image;
	background-image: arrow-left.png;
}

#cmdnext_img
{
	background-decorator: image;
	background-image: arrow-right.png;
}

#cmdfirst_img
{
	background-decorator: image;
	background-image: skip-l.png;
}

button#cmdlast_btn:hover #cmdlast_img
{
	background-decorator: image;
	background-image: skip-r-h.png;
}

button#cmdprev_btn:hover #cmdprev_img
{
	background-decorator: image;
	background-image: arrow-left-h.png;
}
button#cmdpause_btn:hover #cmdpause_img
{
	background-decorator: image;
	background-image: pause-h.png;
}

button#cmdnext_btn:hover #cmdnext_img
{
	background-decorator: image;
	background-image: arrow-right-h.png;
}

button#cmdfirst_btn:hover #cmdfirst_img
{
	background-decorator: image;
	background-image: skip-l-h.png;
}

button#cmdlast_btn:active #cmdlast_img
{
	background-decorator: image;
	background-image: skip-r-o.png;
}

button#cmdprev_btn:active #cmdprev_img
{
	background-decorator: image;
	background-image: arrow-left-o.png;
}

button#cmdpause_btn:active #cmdpause_img
{
	background-decorator: image;
	background-image: pause-o.png;
}

button#cmdnext_btn:active #cmdnext_img
{
	background-decorator: image;
	background-image: arrow-right-o.png;
}

button#cmdfirst_btn:active #cmdfirst_img
{
	background-decorator: image;
	background-image: skip-l-o.png;
}

