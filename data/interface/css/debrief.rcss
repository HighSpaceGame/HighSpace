/*Scroll Buttons*/

#debrief_text scrollbarvertical
{
	scrollchange-sound: 9;
	width: 7%;
	position: absolute;
	top: 21%;
	left: -11.1%;
	z-index: 5;
}

#debrief_text scrollbarvertical slidertrack
{
	visibility: hidden;
}

#debrief_text scrollbarvertical sliderbar
{
	visibility: hidden;
}

#debrief_text scrollbarvertical sliderbar:hover
{
	
}

#debrief_text scrollbarvertical sliderbar:active
{
	
}

#debrief_text scrollbarvertical sliderarrowdec
{
	width: 100%;
	height: 11%;
	margin-top: 2%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-up.png;
}

#debrief_text scrollbarvertical sliderarrowinc
{
	width: 100%;
	height: 11%;
	margin-bottom: 75%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-down.png;
}

#debrief_text scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-dbl-up-h.png;
}

#debrief_text scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-dbl-down-h.png;
}

#debrief_text scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-dbl-up-o.png;
}

#debrief_text scrollbarvertical sliderarrowinc:active
{
	icon-image: arrow-dbl-down-o.png;
}
