body
{
	background-decorator: image;
	background-image: bg.png;
	z-index: bottom;
	z-index: -1;
}

.main_bg
{
	background-decorator: image;
	background-image: bg-cockpit.png;
}

.below p
{
	padding-top:8%;
}

ul#list_times_ul
{
	float: left;
	width: 10%;
}

div#hud_config_wrapper
{
	width: 60%;
	height: auto;
	position: absolute;
	right: 0%;
}

.hud_configlist_element
{
	display: block;
	color: #7b7b7b;
}
.hud_configlist_element:hover
{
	color: #abbbab;
}

div#color_lock
{
	width: 66%;
	height: 100%;
	position: absolute;
}

.locked
{
	z-index: 7;
}

.unlocked
{
	z-index: -1;
}

.hud_config_header
{
	text-align: center;
}

.name_display
{
	width: 40%;
	float: left;
}

.name_display:hover
{
	color: #abbbab;
}
.name_display:checked
{
	color: #4e629c;
}
.name_display:enabled
{
	color: #6f92f5;
}

div#log_text_wrapper
{
	position: absolute;
	top: 12%;
	width: 87%;
	height: 75%;
	margin-left: 7%;
	overflow: scroll;
}

div#preset_text_wrapper
{
	position: absolute;
	top: 12%;
	width: 87%;
	height: 75%;
	margin-left: 2%;
	overflow: hidden;
}

div#hud_config_content
{
	margin-top: 2%;
	width: 100%;
	height: auto;
}

div#hud_config_content img
{
	display: block;
	width: 100%;
	height: auto;
}

div#hud_drawn_content
{
	margin-left: 1%;
	margin-top: 3%;
	width: 93%;
	height: 93%;
	position: absolute;
}

/*ColorPanel Elements*/

div#color_panel_wrapper
{
	width: 25%;
	height: auto;
	overflow: hidden;
	position: absolute;
	top: 4%;
	left: 20%;
}

div#color_panel
{
	width: 66%;
	height: auto;
	position: relative;
	margin-top: 15px;
}

div#color_panel img
{
	display: block;
