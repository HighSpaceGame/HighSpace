/*Scroll Buttons*/

#log_text_wrapper scrollbarvertical
{
	scrollchange-sound: 9;
	width: 7%;
	position: absolute;
	right: 1%;
	z-index: 5;
}

#log_text_wrapper scrollbarvertical slidertrack
{
	display:none;
}

#log_text_wrapper scrollbarvertical sliderbar
{
	display:none;
}

#log_text_wrapper scrollbarvertical sliderbar:hover
{
	display:none;
}

#log_text_wrapper scrollbarvertical sliderbar:active
{
	display:none;
}

#log_text_wrapper scrollbarvertical sliderarrowdec
{
	width: 80%;
	height: 16%;
	margin-top: 9%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-up.png;
}

#log_text_wrapper scrollbarvertical sliderarrowinc
{
	width: 80%;
	height: 16%;
	margin-bottom: 9%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-down.png;
}

#log_text_wrapper scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-dbl-up-h.png;
}

#log_text_wrapper scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-dbl-down-h.png;
}

#log_text_wrapper scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-dbl-up-o.png;
}

#log_text_wrapper scrollbarvertical sliderarrowinc:active
{
	icon-image: arrow-dbl-down-o.png;
}

/*Range Sliders*/

#range_cont_r
{
	width: 10%;
	height: 100%;
	margin-left: 10%;
	z-index: 5;
	position: absolute;
}

#range_cont_g
{
	width: 10%;
	height: 100%;
	margin-left: 33%;
	z-index: 5;
	position: absolute;
}

#range_cont_b
{
	width: 10%;
	height: 100%;
	margin-left: 56%;
	z-index: 5;
	position: absolute;
}

#range_cont_a
{
	width: 10%;
	height: 100%;
	margin-left: 79.3%;
	z-index: 5;
	position: absolute;
}

input.range
{
	width: 100%;
	height: 100%;
	scrollchange-sound: 9;
}

input.range slidertrack
{
	background-decorator: image;
	background-image: scroll-bar-line-horz.png;
	height: 100%;
	width: 1%;
}

input.range sliderbar
{
	width: 100%;
	height: 5%;

	background-decorator: image;
	background-image: scroll-button.png;

}

input.range sliderbar:hover
{
	background-image: scroll-button-h.png;
}

input.range sliderbar:active
{
	background-image: scroll-button-o.png;
}

input.range sliderarrowdec
{
	width: 100%;
	height: 5%;
	margin-top: 2%;
	margin-bottom: 1%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-up.png;
}

input.range sliderarrowinc
{
	width: 100%;
	height: 5%;
	margin-top: 1%;
	margin-bottom: 2.5%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-down.png;
}

input.range sliderarrowdec:hover
{
	icon-image: arrow-up-h.png;
}

input.range sliderarrowinc:hover
{
	icon-image: arrow-down-h.png;
}

input.range sliderarrowdec:active
{
	icon-image: arrow-up-o.png;
}

input.range sliderarrowinc:active
{
	icon-image: arrow-down-o.png;
}

