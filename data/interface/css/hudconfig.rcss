}

/*New Button*/

div#new_cont
{
	width: 20%;
	height: auto;
	float: left;
	margin-left: 10%;
}

.new
{
	display: block;
	width: 100%;
	height: auto;

}

#new_img
{
	background-decorator: image;
	background-image: star.png;
}

#new_btn
{
	click-sound: (error)15,(success)7;
}

button#new_btn:hover #new_img
{
	background-decorator: image;
	background-image: star-pos-h.png;
}

button#new_btn:active #new_img
{
	background-decorator: image;
	background-image: star-pos-o.png;
}

button#new_btn:checked #new_img
{
	background-decorator: image;
	background-image: star-pos-o.png;
}

