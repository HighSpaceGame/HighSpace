}

button#cloneplayer_btn:active #cloneplayer_img
{
	background-decorator: image;
	background-image: clone-pilot-o.png;
}

/*Delete Player Button*/

.deleteplayer
{
	display: block;
	width: 100%;
	height: auto;

}

#deleteplayer_img
{
	background-decorator: image;
	background-image: delete-pilot.png;
}

button#deleteplayer_btn:hover #deleteplayer_img
{
	background-decorator: image;
	background-image: delete-pilot-h.png;
}

button#deleteplayer_btn:active #deleteplayer_img
{
	background-decorator: image;
	background-image: delete-pilot-o.png;
}

/*Pilot Head Select Window Elements*/

div#pilot_head_select
{
	width: 25%;
	height: auto;
	overflow: hidden;
	position: relative;
	float: right;
}

div#pilot_head_select img
{
	display: block;
	width: 100%;
	height: auto;
}

div#pilot_head_images
{
	position: absolute;
	top: 20%;
	margin: auto;
	width: 60%;
}

div#pilot_head_counter
{
	position: absolute;
	bottom: 10%;
	left: 10%;
}

/*LR Arrow Buttons*/

div#lr_arrows_wrapper
{
	position: absolute;
	bottom: 0;
	margin-right: 1%;
}

div#lr_arrows_cont
{
	float: right;
	width: 12%;
	height: auto;
	margin-right: 5%;

}

.lr_arrows
{
	display: block;
	width: 100%;
	height: auto;

}

.arrow_left_img
{
	background-decorator: image;
	background-image: arrow-left.png;
}

.arrow_right_img
{
	background-decorator: image;
	background-image: arrow-right.png;
}

button.arrow_left_btn:hover .arrow_left_img
{
	background-decorator: image;
	background-image: arrow-left-h.png;
}

button.arrow_right_btn:hover .arrow_right_img
{
	background-decorator: image;
	background-image: arrow-right-h.png;
}

button.arrow_left_btn:active .arrow_left_img
{
	background-decorator: image;
	background-image: arrow-left-o.png;
}

button.arrow_right_btn:active .arrow_right_img
{
	background-decorator: image;
	background-image: arrow-right-o.png;
}

/*Pilot Squad Select Window Elements*/

div#pilot_squad_select
{
	width: 18%;
	height: auto;
	overflow: hidden;
	position: relative;
	float: right;
}

div#pilot_squad_select img
{
	display: block;
	width: 100%;
	height: auto;
}

div#pilot_squad_images
{
	position: absolute;
	top: 15%;
	margin-left: 15%;
	width: 60%;
}

div#pilot_squad_counter
{
	position: absolute;
	bottom: 10%;
	left: 10%;
}

/*LR Arrow Buttons*/

div#lr_arrows_wrapper_2
{
	position: absolute;
	bottom: 0;
	margin-right: 1%;
}

div#lr_arrows_cont_2
{
	float: right;
	width: 15%;
	height: auto;
	margin-right: 7%;

}

/*Pilot Info Window Elements*/

div#pilot_info
{
	width: 45%;
	height: auto;
	overflow: hidden;
	position: relative;
	float: left;
}

div#pilot_info img
{
	display: block;
	width: 100%;
	height: auto;
}

div#pilot_info_title
{
	margin-top: 2%;
	margin-left: 4%;
}

div#pilot_stats_text
{
	position: absolute;
	top: 0;
	margin-top: 10%;
	margin-left: 13%;
	margin-right: 5%;
	overflow: scroll;
	height: 85%;
}

/*Stats Text*/
.stats_heading
{
	text-align: center;
}

.stats_value_label
{
	float: left;
	width: 50%;
	text-align: right;
}

.stats_value_text
{
	float: right;
	width: 49%;
	padding-left: 1%;
}

/*Scroll Buttons*/

div#stats_scroll_wrapper
{
	position: absolute;
	top: 16%;
}

div#stats_scroll_cont
{
	width: 8%;
	height: auto;
}

.stats_scroll_btns
{
	display: block;
	width: 100%;
	height: auto;
	visibility: hidden;

}

#scroll_up_img
{
	background-decorator: image;
	background-image: arrow-dbl-up.png;
}

#scroll_down_img
{
	background-decorator: image;
	background-image: arrow-dbl-down.png;
}

button#scroll_up_btn:hover #scroll_up_img
{
	background-decorator: image;
	background-image: arrow-dbl-up-h.png;
}

button#scroll_down_btn:hover #scroll_down_img
{
	background-decorator: image;
	background-image: arrow-dbl-down-h.png;
}

button#scroll_up_btn:active #scroll_up_img
{
	background-decorator: image;
	background-image: arrow-dbl-up-o.png;
}

button#scroll_down_btn:active #scroll_down_img
{
	background-decorator: image;
	background-image: arrow-dbl-down-o.png;
}

/*Medals Button*/

div#medals_btn_wrapper
{
	position: absolute;
	bottom: 3%;
	margin-right: 0.3%;
}

div#medals_btn_cont
{
	width: 8%;
	height: auto;
	float: right;
}

.medals
{
	display: block;
