body
{
	background-decorator: image;
	background-image: bg.png;
	z-index: bottom;
	z-index: -1;
}

.below p
{
	padding-top:8%;
}

div#logo
{
	position: absolute;
	bottom: 7%;
	right: 13%;
	width: 40%;
	height: auto;
	overflow: hidden;
}

div#logo img
{
	display: block;
	width: 100%;
	height: auto;
}

.window_container
{
	margin: 1%;
}

#lower_windows
{
	position: absolute;
	bottom: 0;
}

/*Bottom Panel Elements*/

div#bottom_panel_wrapper
{
	width: 15%;
	height: auto;
	overflow: hidden;
	position: absolute;
	bottom: 0;
	right: 0;
}

div#bottom_panel_a
{
	width: 66%;
	height: auto;
	position: relative;
	float: right;
}

div#bottom_panel_a img
{
	display: block;
	width: 100%;
	height: auto;
}

div#bottom_panel_b
{
	width: 34%;
	height: auto;
	margin-top: 1.1%;
	position: relative;
	float: left;
}

div#bottom_panel_b img
{
	display: block;
	width: 100%;
	height: auto;

}

/*Pilot Select Window Elements*/

div#pilot_select
{
	width: 50%;
	height: auto;
	overflow: hidden;
	position: relative;
	float: left;
}

div#pilot_select img
{
	display: block;
	width: 100%;
	height: auto;
}

div#pilot_select_title
{
	margin-top: 4%;
	margin-left: 4%;
}

div#pilotlist
{
	position: absolute;
	top: 0;
	margin-top: 23%;
	margin-left: 2%;
	margin-right: 5%;
	color: white;
	overflow: auto;
	height: 52%;
}

.pilotlist_element
{
	display: block;
	color: #7b7b7b;
	z-index: 5;
}
.pilotlist_element:hover
{
	color: #abbbab;
}
.pilotlist_element:checked
{
	color: #6f92f5;
}

div#player_select
{
	position: absolute;
	bottom: 0;
}

div#player_type_select
{
	position: absolute;
	top: 0;
}

div#player_control
{
	position: absolute;
	bottom: 0;
}


div#player_control_wrapper
{
	margin-bottom: 4%;
}

/*Arrow Buttons*/

div#arrow_cont
{
	float: right;
	width: 5%;
	height: auto;
	margin-right: 1%;
	margin-bottom: 1%;
	margin-top: 7%;
}

.arrowbuttons
{
	display: block;
	width: 100%;
	height: auto;

}

#playerprev_img
{
	background-decorator: image;
	background-image: arrow-up.png;
}

#playernext_img
{
	background-decorator: image;
	background-image: arrow-down.png;
}

button#playerprev_btn:hover #playerprev_img
{
	background-decorator: image;
	background-image: arrow-up-h.png;
}

button#playernext_btn:hover #playernext_img
{
	background-decorator: image;
	background-image: arrow-down-h.png;
}

button#playerprev_btn:active #playerprev_img
{
	background-decorator: image;
	background-image: arrow-up-o.png;
}

button#playernext_btn:active #playernext_img
{
	background-decorator: image;
	background-image: arrow-down-o.png;
}

/*Commit Button*/

div#player_commit_cont
{
	float: right;
	width: 9%;
	height: auto;
	margin-right: 1%;
	top: 0;
	margin-bottom: -8%;
	visibility: hidden;
}

.playercommit
{
	display: block;
	width: 100%;
	height: auto;

}

#playercommit_img
{
	background-decorator: image;
	background-image: select.png;
}

button#playercommit_btn:hover #playercommit_img
{
	background-decorator: image;
	background-image: select-neg-h.png;
}

button#playercommit_btn:active #playercommit_img
{
	background-decorator: image;
	background-image: select-neg-o.png;
}

/*Singleplayer Selection Button*/

div#single_multi_cont
{
	float: right;
	width: 6%;
	height: auto;
	margin-right: 4%;
	margin-bottom: 1%;
	margin-top: 1%;
}

.singleplayer
{
	display: block;
	width: 100%;
	height: auto;

}

#singleplayer_img
{
	background-decorator: image;
	background-image: single-player.png;
}

#singleplayer_btn:hover #singleplayer_img
{
	background-decorator: image;
	background-image: single-player-h.png;
}

#singleplayer_btn:active #singleplayer_img
{
	background-decorator: image;
	background-image: single-player-o.png;
}

#singleplayer_btn:checked #singleplayer_img
{
	background-decorator: image;
	background-image: single-player-o.png;
}

/*Multiplayer Selection Button*/

.multiplayer
{
	display: block;
	width: 100%;
	height: auto;

}

#multiplayer_img
{
	background-decorator: image;
