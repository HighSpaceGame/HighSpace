}

button#multiplayer_btn:hover #multiplayer_img
{
	background-decorator: image;
	background-image: multiplayer-h.png;
}

button#multiplayer_btn:active #multiplayer_img
{
	background-decorator: image;
	background-image: multiplayer-o.png;
}

button#multiplayer_btn:checked #multiplayer_img
{
	background-decorator: image;
	background-image: multiplayer-o.png;
}

/*Create Player Button*/

div#player_control_cont
{
	float: left;
	width: 6%;
	height: auto;
	margin-right: 1%;
	margin-left: 5%;
	top: 0;
	margin-bottom: -6%;
	z-index: 1;
}

.createplayer
{
	display: block;
	width: 100%;
	height: auto;

}

#createplayer_img
{
	background-decorator: image;
	background-image: create-pilot.png;
}

button#createplayer_btn:hover #createplayer_img
{
	background-decorator: image;
	background-image: create-pilot-h.png;
}

button#createplayer_btn:active #createplayer_img
{
	background-decorator: image;
	background-image: create-pilot-o.png;
}

/*Clone Player Button*/

.cloneplayer
{
	display: block;
	width: 100%;
	height: auto;

}

#cloneplayer_img
{
	background-decorator: image;
	background-image: clone-pilot.png;
}

button#cloneplayer_btn:hover #cloneplayer_img
{
	background-decorator: image;
